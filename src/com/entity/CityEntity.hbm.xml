<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.entity.CityEntity" table="city" schema="atm">
        <id name="cityId">
            <column name="cityId" sql-type="int(100)"/>
            <generator class="increment"/>
        </id>
        <property name="cityName">
            <column name="cityName" sql-type="varchar(10)" length="10"/>
        </property>
    </class>
</hibernate-mapping>